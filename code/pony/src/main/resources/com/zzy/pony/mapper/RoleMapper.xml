<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zzy.pony.mapper.RoleMapper">
	<select id="findRoleCodeByUser" resultType="string">
		select ROLE_CODE from t_user_role where user_id=#{0}
	</select>
	
</mapper>